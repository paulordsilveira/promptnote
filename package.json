    "dev:server": "nodemon backend/server.mjs",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "clean": "rimraf node_modules frontend/node_modules database.sqlite sessions.sqlite",
    "prepare": "husky"
